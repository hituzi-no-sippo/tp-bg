{
  "problemMatcher": [
    {
      "owner": "editorconfig-checker",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*):$",
          "file": 1
        },
        {
          "regexp": "^\\t(\\d+):\\s(.*)$",
          "line": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
